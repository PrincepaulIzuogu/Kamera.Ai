{
    "name": "Kamera.Ai",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "frontend",
    "workspaceFolder": "/app",
    "extensions": [
        "ms-azuretools.vscode-docker"
    ],
    "settings": {
        "docker.host": "tcp://localhost:2375",
        "docker.defaultNetwork": "bridge"
    },
    "postCreateCommand": "apt-get update && apt-get install -y bash curl jq docker.io docker-compose",
    "postStartCommand": "docker-compose up --build"
}
